<script src="https://unpkg.com/aos@next/dist/aos.js" is:inline></script>
<script is:inline>
  AOS.init({
    offset: 80,
    delay: 0,
    duration: 700,
    easing: "ease",
    once: false,
  });
  document.addEventListener("astro:after-swap", () => {
    AOS.init({
      offset: 80,
      delay: 0,
      duration: 700,
      easing: "ease",
      once: false,
    });
  });
</script>
